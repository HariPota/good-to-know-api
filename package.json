{
  "name": "good-to-know-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "env $(cat .env.dev | grep -v \\\"#\\\" | xargs) concurrently \"npx tsc --watch\" \"nodemon -q dist/src/index.js\"",
    "db:schema:update": "node dist/config/sync-database.js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "test:unit": "env $(cat .env.test | grep -v \\\"#\\\" | xargs) jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HariPota/good-to-know-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HariPota/good-to-know-api/issues"
  },
  "homepage": "https://github.com/HariPota/good-to-know-api#readme",
  "_moduleAliases": {
    "@": "dist",
    "@src": "dist/src"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mariadb": "^3.0.2",
    "sequelize": "^6.25.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
